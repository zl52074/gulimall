server:
  port: 8000 # 服务端口
spring:
  application:
    name: gulimall-product # 服务名
  datasource:
    username: root
    password: root
    url: jdbc:mysql://localhost:3306/gulimall_pms
    driver-class-name: com.mysql.cj.jdbc.Driver
  cloud:
    nacos:
      discovery:
        server-addr: localhost:8848 # nacos服务地址

mybatis-plus:
  mapper-locations: classpath:/mapper/**/*.xml
  global-config:
    db-config:
      id-type: auto
